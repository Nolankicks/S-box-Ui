@using Sandbox;
@using Sandbox.UI;
@inherits PanelComponent


<root>
	<button @onclick=@changeScene>Play Platformer</button>
</root>




@code
{

	[Property] public SceneFile newScene { get; set; } //allows the scene to be change in the editor

	
	void changeScene() //change scene void to load the newScene defined above
	{
		Log.Info("aaa");
		GameManager.ActiveScene.Load(newScene);
	}	

	protected override int BuildHash() => System.HashCode.Combine( changeScene ); //does some hash shit idk


	





	
	
}
